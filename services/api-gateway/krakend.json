{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-metrics": {
      "listen_address": ":8090"
    },
    "github_com/devopsfaith/krakend-gologging": {
      "level": "DEBUG",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true
    }
  },
  "endpoints": [
    {{ template "endpoint.tmpl" .article }},
    {{ template "endpoint.tmpl" .cart }},
    {{ template "endpoint.tmpl" .documentation }}
  ]
}