{
  "hosts": [
    "http://auth_app_1:3500"
  ],
  "endpoints": [
    {
      "endpoint": "/auth/login",
      "method": "POST",
      "url_pattern": "/auth/login",
      "encoding": "json",
      "extra_config": {
        "github.com/devopsfaith/krakend-jose/signer": {
          "alg": "HS256",
          "kid": "micro-express",
          "keys-to-sign": [
            "accessToken"
          ],
          "jwk-url": "http://auth_app_1:3500/symmetric.json",
          "disable_jwk_security": true
        }
      }
    },
    {
      "endpoint": "/auth/sign-up",
      "method": "PUT",
      "url_pattern": "/auth/sign-up",
      "encoding": "no-op",
      "extra_config": {}
    }
  ]
}