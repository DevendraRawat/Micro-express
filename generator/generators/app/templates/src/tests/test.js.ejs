    if (params.length === 1) {
      if (params[0].type === 'integer' || params[0].type === 'double') {
        numberIndexes.push(0);
      }
      cases.push({'eeeoooo': 'aboer'});
    } else {
      for (let i = 0; i < params.length; i++) {
        let data = {};
        for (let j = 0; j < params.length; j++) {
          if (j !== i) {
            let currentValue = "";
