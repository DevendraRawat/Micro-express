    const data = await prisma.create<%= resourceName -%>(sampleOk);<% } %>
    const res = await request(app)
      .<%= requestMethod %>(<% if (hasIdInPath) { %>`<%= presentIdRoute %>`<% } else {%>'<%= route %>'<% } %>)<% if (!requestBody) { %>;<% } else { %>
